@article{chenSaponinsIsolatedRoot2008,
  title = {Saponins Isolated from the Root of {{Panax}} Notoginseng Showed Significant Anti-Diabetic Effects in {{KK-Ay}} Mice},
  author = {Chen, Zhong-Hua and Li, Jie and Liu, Jie and Zhao, Yuan and Zhang, Pu and Zhang, Meng-Xue and Zhang, Liu},
