@article{liuSaponinsPanaxNotoginseng2014,
  title = {Saponins of {{Panax}} Notoginseng: Chemistry, Cellular Targets and Therapeutic Opportunities in Cardiovascular Diseases.},
  author = {Liu, Jingjing and Wang, Yitao and Qiu, Lan and Yu, Yuanyuan and Wang, Chunming},
