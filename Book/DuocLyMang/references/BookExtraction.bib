@article{blume-peytaviAgeAssociatedSkinConditions2016,
  title = {Age-{{Associated Skin Conditions}} and {{Diseases}}: {{Current Perspectives}} and {{Future Options}}},
  author = {Blume-Peytavi, Ulrike and Kottner, Jan and Sterry, Wolfram and Hodin, Michael W and Griffiths, Tamara W and Watson, Rachel E B and Hay, Roderick J and Griffiths, Christopher E M},
