  doi = {10.1096/fasebj.13.13.1796},
  abstract = {The infectious hepatitis B virus represents 42 nm spherical double-shelled particles. However, analysis of blood from hepatitis B virus carriers revealed the presence of smaller 22 nm particles consisting of a viral envelope surface protein. These particles are highly immunogenic and have been used in the design of hepatitis B virus vaccine produced in yeast. Upon expression in yeast, these proteins form virus-like particles that are used for parenteral immunization. Therefore, the DNA fragment encoding hepatitis B virus surface antigen was introduced into Agrobacterium tumerifacience LBA4404 and used to obtain transgenic lupin (Lupinus luteus L.) and lettuce (Lactuca sativa L.) cv. Burpee Bibb expressing envelope surface protein. Mice that were fed the transgenic lupin tissue developed significant levels of hepatitis B virus-specific antibodies. Human volunteers, fed with transgenic lettuce plants expressing hepatitis B virus surface antigen, developed specific serum-IgG response to plant produced protein.},
  isbn = {0892-6638 (Print)},
  keywords = {feasible approach to,genic lupin,hbsag,lettuce and engineered plants,of vaccines,oral or intranasal administration,oral vaccines,remains the only economically,surface antigen,trans-}
}

