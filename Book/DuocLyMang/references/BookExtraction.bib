@article{bellIsolationGoxalylDerivatives1966,
  title = {The Isolation of α- and γ-Oxalyl Derivatives of α,γ-Diaminobutyric Acid from Seeds of {{Lathyrus}} Latifolius, and the Detection of the α-Oxalyl Isomer of the Neurotoxin α-Amino-β-Oxalylaminopropionic Acid Which Occurs Together with the Neurotoxin in This And},
