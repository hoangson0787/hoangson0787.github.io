  volume = {35},
  number = {2},
  eprint = {32860651},
  eprinttype = {pmid},
  pages = {657--679},
  issn = {1099-1573},
  doi = {10.1002/ptr.6854},
  abstract = {Essential oils have been used as remedies since ancient times for the treatment of numerous illnesses on account of their wide range of biological activities. Recent preclinical and clinical studies have shown varying pharmacological responses in the nervous system leading to anxiolytic, antidepressant, sedative, and anticonvulsant effects. Experimentation in animal models has evidenced the involvement of multiple neurotransmitter systems in the mode of action of essential oils, resulting in measurable physiological effects in the brain. Additionally, clinical trials have demonstrated the influence of essential oils in physiological parameters such as blood pressure, heart rate, respiratory rate, brain waves composition, and cortisol serum levels with concomitant psychological effects. Although there is growing evidence of measurable effects of essential oils in animal brains, more clinical research is required to validate their influence in the human central nervous system. This will enable the development of essential oil-based drugs for the treatment of mental illnesses such as depression, anxiety and dementia.},
  langid = {english},
  keywords = {Animals,Central Nervous System,clinical trials,essential oils,Humans,Mental Health,molecular pathways,nervous system,neuropharmacology,{Oils, Volatile},Phytochemicals,Phytotherapy},
  file = {/Users/hoangleson/Zotero/storage/XDD8VIHX/Lizarraga-Valderrama - 2021 - Effects of essential oils on central nervous syste.pdf;/Users/hoangleson/Zotero/storage/KJLJTV2P/ptr.html}
}

@article{logieProtonNMRSpectroscopy1994,
  title = {Proton {{NMR}} Spectroscopy of Pyrrolizidine Alkaloids},
  author = {Logie, Catherine G. and Grue, M.Ruth and Liddell, J.Richard},
  date = {1994},
  journaltitle = {Phytochemistry},
