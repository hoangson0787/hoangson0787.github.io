@article{khurana2017elucidation,
  title = {Elucidation of Stress-Induced Degradation Products of Mangiferin: {{Method}} Development and Validation},
  author = {Khurana, Rajneet Kaur and Kaur, Satvinder and Kaur, Jasleen and Singh, Bhupinder},
  date = {2017-08-01},
  journaltitle = {Biomedical Chromatography},
  volume = {31},
  number = {8},
  eprint = {28090677},
  eprinttype = {pmid},
  pages = {e3935},
  publisher = {{Wiley Online Library}},
  issn = {1099-0801},
  doi = {10.1002/BMC.3935},
  url = {https://pubmed.ncbi.nlm.nih.gov/28090677/},
  urldate = {2022-01-27},
  abstract = {The degradation behavior of mangiferin, under various ICH Q1A(R2) recommended stress conditions, was studied using an isocratic elution with mobile phase (pH 2.4), composed of acetonitrile and 1\% orthophosphoric acid (12:88 v/v) at a flow rate of 1.0 mL/min, with λmax 262 nm. It was suitably adapted for LC–MS studies by replacing with 1\% acetic acid (ACN–1\% acetic acid; 18:82) and the pH was adjusted to 3.0. Extensive degradation was found to occur during alkaline medium stress studies at 2.31 min of retention time at λmax of 235 nm. The mass spectrum of mangiferin, 3 h after treatment with 0.1 M NaOH, clearly shows the rupture of the tricyclic ring, indicating that a fragment at m/z − 269 was formed. Furthermore, the results were supported by nuclear magnetic resonance as well. However, no degradation was observed in other stress conditions.},
  keywords = {Anti-Inflammatory Agents / chemistry*,Antioxidants / chemistry*,Bhupinder Singh,Chromatography,doi:10.1002/bmc.3935,Drug Stability,Hydrolysis,Liquid,Magnetic Resonance Spectroscopy,Mangifera / chemistry,MEDLINE,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,NCBI,NIH,NLM,Plant Extracts / chemistry*,pmid:28090677,PubMed Abstract,Rajneet Kaur Khurana,Satvinder Kaur,Tandem Mass Spectrometry,Xanthones / chemistry*}
}

