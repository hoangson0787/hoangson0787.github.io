@article{liLitseaCubebaEssential2022,
  title = {\emph{Litsea Cubeba} {{Essential Oil}}: {{Component Analysis}}, {{Anti-}}{{\emph{Candida}}}\emph{ Albicans} {{Activity}} and {{Mechanism Based}} on {{Molecular Docking}}},
  shorttitle = {\emph{Litsea Cubeba} {{Essential Oil}}},
