@article{xieAdjuvantEffectAtractylodis2012,
  title = {Adjuvant Effect of {{{\emph{Atractylodis}}}}{\emph{ Macrocephalae}} {{Koidz}}. Polysaccharides on the Immune Response to Foot-and-Mouth Disease Vaccine},
  author = {Xie, Feng and Li, Yutao and Su, Fei and Hu, Songhua},
