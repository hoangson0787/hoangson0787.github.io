  author = {Bodensieck, Antje and Fabian, Walter M.F. and Kunert, Olaf and Belaj, Ferdinand and Jahangir, Sajid and Schühly, Wolfgang and Bauer, Rudolf},
  date = {2010},
  journaltitle = {Chirality},
