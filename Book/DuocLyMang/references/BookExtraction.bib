@article{matkowskiMangiferinBioactiveXanthonoid2013,
  title = {Mangiferin - a Bioactive Xanthonoid, Not Only from Mango and Not Just Antioxidant},
  author = {Matkowski, Adam and Kuś, Piotr and Góralska, Edyta and Woźniak, Dorota},
  date = {2013},
  journaltitle = {Mini Reviews in Medicinal Chemistry},
  volume = {13},
