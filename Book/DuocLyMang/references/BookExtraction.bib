@article{wangArabinogalactanFlowersPanax2015,
  title = {An Arabinogalactan from Flowers of {{Panax}} Notoginseng Inhibits Angiogenesis by {{BMP2}}/{{Smad}}/{{Id1}} Signaling},
  author = {Wang, Peipei and Zhang, Lei and Yao, Jian and Shi, Yikang and Li, Ping and Ding, Kan},
