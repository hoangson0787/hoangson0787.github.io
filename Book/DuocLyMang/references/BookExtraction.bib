@article{sahinGABAergicEffectsFoods2021,
  title = {{{GABAergic}} Effects of Some Foods Extracts via Inhibition of {{GABA}} Transaminase ({{GABA-T}})},
  author = {Şahi̇N, Sümeyye and Haas, Sahra},
  date = {2021-12-31},
  journaltitle = {Middle Black Sea Journal of Health Science},
  volume = {7},
  number = {3},
  pages = {423--428},
  issn = {2149-7796},
