@article{eringaEndothelialDysfunctionPre2013,
  title = {Endothelial Dysfunction in (Pre)Diabetes: {{Characteristics}}, Causative Mechanisms and Pathogenic Role in Type 2 Diabetes},
  author = {Eringa, Etto C. and Serne, Erik H. and Meijer, Rick I. and Schalkwijk, Casper G. and Houben, Alfons J H M and Stehouwer, Coen D A and Smulders, Yvo M. and Van Hinsbergh, Victor W M},
  date = {2013},
  journaltitle = {Reviews in Endocrine and Metabolic Disorders},
  volume = {14},
