  author = {Nakai, Yoichiro and Kido, Toshitaka and Hashimoto, Kazunori and Kase, Yoshio and Sakakibara, Iwao and Higuchi, Masami and Sasaki, Hiroshi},
  date = {2003},
  journaltitle = {Journal of Ethnopharmacology},
