@article{anbarasiEffectBacosideBrain2006,
  title = {Effect of Bacoside {{A}} on Brain Antioxidant Status in Cigarette Smoke Exposed Rats},
  author = {Anbarasi, K. and Vani, G. and Balakrishna, K. and Devi, C. S.Shyamala},
  date = {2006},
  journaltitle = {Life Sciences},
  volume = {78},
  number = {12},
  eprint = {16226278},
  eprinttype = {pmid},
  pages = {1378--1384},
  issn = {00243205},
  doi = {10.1016/j.lfs.2005.07.030},
  abstract = {Free radicals mediated oxidative stress has been implicated in the pathogenesis of smoking-related diseases and antioxidant nutrients are reported to prevent the oxidative damage induced by smoking. Therefore, the present study was conducted to evaluate the antioxidant role of bacoside A (triterpenoid saponin isolated from Bacopa monniera) against chronic cigarette smoking induced oxidative damage in rat brain. Adult male albino rats were exposed to cigarette smoke for a period of 12 weeks and simultaneously administered with bacoside A (10 mg/kg b.w./day, p.o.). Antioxidant status of the brain was assessed from the levels of reduced glutathione, vitamin C, vitamin E, and vitamin A and the activities of superoxide dismutase, catalase, glutathione peroxidase and glutathione reductase. The levels of copper, iron, zinc and selenium in brain and serum ceruloplasmin activity were also measured. Oxidative stress was evident from the diminished levels of both enzymatic and non-enzymatic antioxidants. Alterations in the levels of trace elements with accumulation of copper and iron, and depletion of zinc and selenium were also observed. Bacoside A administration improved the antioxidant status and maintained the levels of trace elements. These results suggest that chronic cigarette smoke exposure enhances oxidative stress, thereby disturbing the tissue defense system and bacoside A protects the brain from the oxidative damage through its antioxidant potential. © 2005 Elsevier Inc. All rights reserved.},
  isbn = {0024-3205},
  keywords = {Antioxidants,Bacopa monniera,Bacoside A,Cigarette smoking,Lipid peroxidation,Oxidative stress,Trace elements}
}

