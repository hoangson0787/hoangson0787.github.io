@incollection{wangRecentAdvancesDiscovery1998,
  title = {Recent {{Advances}} in the {{Discovery}} and {{Development}} of {{Flavonoids}} and Their {{Analogues}} as {{Antitumor}} and {{Anti-HIV Agents}}},
  author = {Wang, Hui-Kang and Xia, Yi and Yang, Zheng-Yu and Morris Natschke, Susan L. and Lee, Kuo-Hsiung},
  date = {1998},
  doi = {10.1007/978-1-4615-5335-9_15},
  abstract = {Antitumor and anti-HIV flavonoids and their analogues will be reviewed\textbackslash nwith emphasis on those discovered in our laboratory. The active antitumor\textbackslash ncompounds include the antileukemic tricin (1) and kaempferol-3-O-beta-D-glucopyranoside\textbackslash n(2) from Wikstroemia indica, the cytotoxic hinokiflavone (3) from\textbackslash nRhus succedanea, the cytotoxic isoflavone (8) from Amorpha fruticosa,\textbackslash ntwo dihydroxypentamethoxyflavones (9, 10) from Polanisia dodencandra.\textbackslash nThe development of synthetic 2-phenyl-4-quinolones as potent cytotoxic\textbackslash nantimitotic flavonoid analogues and 2-phenylthiochromen-4-ones as\textbackslash npotent antitumor flavonoid analogues will be presented. Selected\textbackslash nresults from other laboratories and antitumor-related biological\textbackslash nstudies also will be discussed. Flavonoids have also been investigated\textbackslash nas potential anti-HIV agents. In our laboratory, acacetin-7-O-beta-D-galactopyranoside\textbackslash n(131) from Chrysanthemum morifolium and chrysin (102), as well as\textbackslash napigenin-7-O-beta-D-glucopyranoside (130), from Kummerowia striata,\textbackslash nhave been found to exhibit anti-HIV activity. In other studies, some\textbackslash nflavonoids and related compounds have been investigated as inhibitors\textbackslash nof HIV-1 reverse transcriptase, protease, and integrase. The isolation\textbackslash nand structural modification of such plant-derived active principles\textbackslash nprovide a continuing source of potential antitumor and anti-HIV agents.}
}

