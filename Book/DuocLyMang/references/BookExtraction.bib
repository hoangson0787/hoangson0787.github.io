  keywords = {Acanthaceae,Acanthaceae: chemistry,{Antineoplastic Agents, Phytogenic},{Antineoplastic Agents, Phytogenic: chemistry},{Antineoplastic Agents, Phytogenic: isolation \& pur},{Antineoplastic Agents, Phytogenic: pharmacology},{Cell Line, Tumor},Cell Survival,Cell Survival: drug effects,Humans,Lignans,Lignans: chemistry,Lignans: isolation \& purification,Lignans: pharmacology,Neoplasms,Neoplasms: drug therapy,Plant Roots,Plant Roots: chemistry,Triterpenes,Triterpenes: chemistry,Triterpenes: isolation \& purification,Triterpenes: pharmacology}
