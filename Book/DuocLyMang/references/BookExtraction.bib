@article{messingerInvivoSilencingCav32009,
  title = {In-Vivo Silencing of the {{Cav3}}.2 {{T-type}} Calcium Channels in Sensory Neurons Alleviates Hyperalgesia in Rats with Streptozocin-Induced Diabetic Neuropathy},
  author = {Messinger, Richard B and Naik, Ajit K and Jagodic, Miljen M and Nelson, Michael T and Lee, Woo Yong and Choe, Won Joo and Orestes, Peihan and Latham, Janelle R and Todorovic, Slobodan M and Jevtovic-Todorovic, Vesna},
  date = {2009},
  journaltitle = {Pain},
  volume = {145},
  number = {1-2},
  pages = {184--195},
  doi = {10.1016/j.pain.2009.06.012},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2735619/},
  abstract = {Earlier, we showed that streptozocin (STZ)-induced type 1 diabetes in rats leads to the development of painful peripheral diabetic neuropathy (PDN) manifested as thermal hyperalgesia and mechanical allodynia accompanied by significant enhancement of T-type calcium currents (T-currents) and cellular excitability in medium-sized dorsal root ganglion (DRG) neurons. Here, we studied the in-vivo and in-vitro effects of gene-silencing therapy specific for the Cav3.2 isoform of T-channels, on thermal and mechanical hypersensitivity, and T-current expression in small and medium-size DRG neurons of STZ-treated rats. We found that silencing of the T-channel Cav3.2 isoform using antisense oligonucleotides, had a profound and selective anti-hyperalgesic effect in diabetic rats and is accompanied by significant down-regulation of T-currents in DRG neurons. Anti-hyperalgesic effects of CaV3.2 antisense oligonucleotides in diabetic rats were similar in models of rapid and slow onset of hyperglycemia following intravenous and intraperitoneal injections of STZ, respectively. Furthermore, treatments of diabetic rats with daily insulin injections reversed T-current alterations in DRG neurons in parallel with reversal of thermal and mechanical hypersensitivity in-vivo. This confirms that CaV3.2 T-channels, important signal amplifiers in peripheral sensory neurons, may contribute to the cellular hyperexcitability that ultimately leads to the development of painful PDN.},
  keywords = {Administration,alpha Subunit,Amputation,Animal,Animals,Antioxidants,Blood Glucose,Cell Proliferation,Cellulitis,COMBINE,CoRIA,Costs and Cost Analysis,database mining,Diabetes Complications,Diabetes Mellitus,Diabetic Foot,Diabetic Neuropathies,Disease Models,docking,dyslipidemia,Experimental,Foot,Glucosides,Humans,Hyperbaric Oxygenation,Hyperinsulinism,Hypoglycemic Agents,Hypoxia,Hypoxia-Inducible Factor 1,Inbred C57BL,Insulin,Insulin Resistance,Insulin sensitivity,Leg,Magnoliopsida,Male,Mangifera,mangiferin,Mice,Models,natural xanthone,Neovascularization,nephropathy,Oral,Osteomyelitis,pharmacophore,Physiologic,Phytotherapy,Plant Extracts,Plant Leaves,Prevalence,Statistical,Toes,Type 1,Type 2,United States,virtual screening,Wound Healing,Xanthenes,Xanthones,α-glucosidase}
}

