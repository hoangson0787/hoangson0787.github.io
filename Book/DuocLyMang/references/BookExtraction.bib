@article{jannuReviewHepatoprotectivePlants2012,
  title = {A Review on Hepatoprotective Plants},
  author = {Jannu, Vishwanath and Baddam, Preetham Goud and Boorgula, Anil Kumar and Jambula, Satwik Reddy},
  date = {2012},
  journaltitle = {International Journal of Drug Development and Research},
  issn = {09759344},
  doi = {10.13040/IJPSR.0975-8232.5(3).690-02},
  abstract = {Liver is a vital organ play a major role in metabolism and excretion of xenobiotics from the body. Liver injury or liver dysfunction is a major health problem that challenges not only health care professionals but also the pharmaceutical industry and drug regulatory agencies. Liver cell injury caused by various toxic chemicals (certain anti-biotic, chemotherapeutic agents, carbon tetrachloride (CCl4), thioacetamide (TAA) etc.), excessive alcohol consumption and microbes is well studied. The available synthetic drugs to treat liver disorders in this condition also cause further damage to the liver. Hence, Herbal drugs have become increasingly popular and their use is wide spread. Herbal medicines have been used in the treatment of liver diseases for a long time. A number of herbal preparations are available in the market. The present review is aimed at compiling data on promising phytochemicals from medicinal plants that have been tested in hepatotoxicity models using modern scientific system.},
  isbn = {9198722603},
  keywords = {Carbon tetrachloride (CCl4),Hepatotoxicity,Medicinal plants,Paracetamol,Thioacetamide}
}

