@article{zhaoPreparativeIsolationPurification2006,
  title = {Preparative Isolation and Purification of Atractylon and Atractylenolide {{III}} from the {{Chinese}} Medicinal Plant {{Atractylodes}} Macrocephala by High-Speed Counter-Current Chromatography},
  author = {Zhao, Chunxia and He, Chaohong},
