	author = {Ding, Yan and Liang, Chun and Nguyen, Huu Tung and Choi, Eun Mi and Kim, Jeong Ah and Kim, Young Ho},
	date = {2010},
	keywords = {4-methoxyl 5-hydroxymethyl benzoic 3-O-??-D-glucop, Acer mandshuricum, Aceraceae, {MC}3T3-E1 cell, Osteoblast differentiation},
