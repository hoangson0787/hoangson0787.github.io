@article{beecham_n_1972-1,
	title = {The n→π cd of αβ-unsaturated lactones},
	volume = {13},
	issn = {00404039},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S004040390184716X},
	doi = {10.1016/S0040-4039(01)84716-X},
	abstract = {The rules relating the sign of the n → π* Cotton effect to the absolute ring geometry in endo αβ-unsaturated lactones are examined in the light of evidence from x-ray structure analyses. It is shown that the sign/chirality relationship, previously proposed for pentenolides, is correct; but that the same relationship holds for butenolides, contrary to the existing rule. The n → π* {CD} of lactones with exo αβ-unnsaturation is discussed. The effect of allylic oxygen on the {CD} of the lowest energy n → π* transition in αβ-unsaturated lactones is also discussed. Finally, evidence from monosubstituted pentenolides with a phenyl ring in the side chain is reviewed and it is suggested that to assume equatorial orientation for the substituent may not always be correct.},
	pages = {1669--1672},
	number = {17},
	journaltitle = {Tetrahedron Letters},
	author = {Beecham, A.F.},
	date = {1972-01},
}

