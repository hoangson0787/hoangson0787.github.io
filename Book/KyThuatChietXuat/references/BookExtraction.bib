@article{hosny_soy_2015-1,
	title = {Soy polysaccharide as a novel superdisintegrant in sildenafil citrate sublingual tablets: Preparation, characterization, and in vivo evaluation},
	issn = {11778881},
	doi = {10.2147/DDDT.S76314},
	abstract = {Sildenafil citrate ({SC}), a drug used to treat erectile dysfunction, is available in tablet form but has three major problems. First, the drug displays inadequate aqueous solubility, which delays the onset of its action. Second, the drug undergoes extensive first-pass metabolism, resulting in a low (40\%) bioavailability. Third, the gastrointestinal effects of {SC} include dyspepsia and a burning sensation. The aim of this research was to prepare {SC} as a sublingual tablet utilizing soy polysaccharide as novel superdisintegrant to mitigate the abovementioned problems. The solubility of {SC} in various hydrophilic carrier solutions was estimated in order to prepare the drug as a coprecipitate. Sublingual tablets were prepared and evaluated for hardness, friability, drug content, wetting time, water absorption ratio, in vitro dispersion time, dissolution rate, and stability study. The pharmacokinetic study of the tablets was carried out on healthy volunteers. The results indicated that the co-precipitation of {SC} with polyvinylpyrollidone K30 enhanced the solubility of {SC} by more than eight folds. The tablet contained 8\% soy polysaccharide as a superdisintegrant and provided a wetting time of 25 seconds, and in vitro dispersion times of 55 seconds. The drug release was found to be 95.6\%. The prepared {SC} sublingual tablet also exhibited a rapid onset of action, and its bioavailability was enhanced 1.68-fold compared with that of the marketed tablets. It can be concluded that {SC} sublingual tablet is a promising formulation that results in higher solubility, faster dispersion and onset of action, higher release rate, and higher systemic bioavailability.},
	journaltitle = {Drug Design, Development and Therapy},
	author = {Hosny, Khaled Mohamed and Mosli, Hisham Ahmed and Hassan, Ali Habiballah},
	date = {2015},
	pmid = {25624751},
	keywords = {Bioavailability, Emcosoy, Erectile dysfunction, Onset of action, Polyvinylpyrrolidone},
}

@article{phung_duc_truyen_nghien_2015-1,
	title = {Nghiên cứu điều chế phức itraconazol – hydroxypropyl – beta - cyclodextrin},
	volume = {55},
	abstract = {Itraconazol ({ITZ}), một thuốc kháng nấm phổ rộng thuộc nhóm triazol, thuộc phân nhóm {II} trong hệ thống phân loại sinh dược học, {ITZ} là một trong những thuốc không tan trong nước điển hình (khoảng 1 ng/ml ở {pH} trung tính và 6 µg / ml ở {pH} 1). Hydroxypropyl-β-cyclodextrin ({HPβCD}) là dẫn xuất hydroxyalkyl của β-cyclodextrin có độ tan trong nước cao, có bề mặt ngoài thân nước và khoang bên trong kỵ nước có khả năng tạo phức với {ITZ}. Phức {ITZ}-{HPβCD} hòa tan trong nước giúp nâng cao sinh khả dụng của {ITZ} trong các dạng bào chế. Đề tài "Nghiên cứu điều chế phức itraconazol hydroxypropyl beta cyclodextrin {ITZ}-{HPβCD}" được thực hiện làm cơ sở để ứng dụng tiếp vào các dạng bào chế để làm tăng tốc độ hòa tan, độ tan của {ITZ}. Phương pháp nghiên cứu Xác định ảnh hưởng của {HPβCD} đến độ tan của {ITZ}. Xác định tỷ lệ tạo phức của {ITZ} với {HPβCD}. Điều chế phức {ITZ}-{HPβCD}: Phương pháp nghiền ướt, phương pháp đồng bay hơi dung môi. Đánh giá phức: Thử độ hòa tan, độ tan, đo phổ ({IR}, {DSC}, 1H-{NMR}). Kết quả nghiên cứu Hydroxypropyl-beta-cyclodextrin có khả năng tạo phức với {ITZ} theo tỷ lệ 1:1; 1:2 và 1:3, đây cũng là tỷ lệ dễ áp dụng nhất trong bào chế các dạng viên vì khối lượng của viên thuốc không tăng lên nhiều. Khả năng tạo phức được chứng minh bằng phổ {IR}, {DSC}, 1H-{NMR} cho thấy có sự tương tác thực sự giữa {ITZ} và {HPβCD}. Tạo phức bằng phương pháp đồng bay hơi dung môi cho độ hòa tan của {ITZ} cao hơn hẳn phương pháp nghiền ướt ở tất cả các thời điểm lấy mẫu. Độ tan của {ITZtrong} dung dịch {HCl} 0,1N: {ITZ} 10-6, phức {ITZ}-{HPβCD} (1:1) 0,73 mg/ml; phức {ITZ}-{HPβCD} (1:2) 1,48 mg/ml; phức {ITZ}-{HPβCD} (1:3) 14,65 mg/ml chứng minh {HPβCD} là chất đầy tiềm năng ứng dụng trong bào chế để làm tăng khả năng hòa tan của các dược chất kém tan.},
	number = {12},
	journaltitle = {Tạp chí Dược học},
	author = {{Phùng Đức Truyền}},
	date = {2015},
	keywords = {vi},
}

