@article{ma_lignans_2017-1,
	title = {Lignans of phyllanthus niruri solid dispersion: A potential alternative gout therapy},
	issn = {18125700},
	doi = {10.3923/ijp.2017.11.21},
	abstract = {© 2017 Hai-Qiu Ma etal. Background: Phyltetralin (1) and phyllanthin (2) are two potent anti-hyperuricemic lignans in the standardized Phyffanthus niruri extract ({PNF}5) yet displayed low oral bioavailability in rats. Objective: The purposes of the current study were to investigate the contributing factors towards the poor bioavailabilities of the lignans in {PNF}5 and increase the bioavailability by developing a formulation method, which can specifically overcome these contributing factors. Materials and Methods: Aqueous solubility, {pH} stability, P-glycoproteins efflux and first-pass metabolism of 1 and 2 in {PNF}5 were evaluated accordingly using in vitro methods. The {PNF}5 was subsequently formulated as Gelucire®44/14-based solid dispersion capsules ({PNF}5-{SDC}) and the oral bioavailabilities of 1 and 2 in {PNF}5-{SDC} were estimated compared to that of the conventional {PN} F5 powder filled capsules ({PNF}5-{PFC}). Results: The study shown that the solubility of {PNF}5 was 0.12±0.02 mg {mL} -1 , {pH} value of {GIT} lumen were not significant affected the lignans absorption (p {\textbackslash}textgreater 0.05). The in vitro absorption of lignans significantly improved by combination with P-glycoprotein inhibitors: Verapamil (p {\textbackslash}textless 0.001) and quinidine(p {\textbackslash}textless 0.05). In addition, there were no significant variation for the amount of 1 (p = 0.4363) and 2 (p = 0.5396) and no metabolites were observed when {PNF}5 incubated with homogenized liver. Moreover, the relative oral bioavailability of the lignans in {PNF}5-{SDC} was increased by 3 fold (p {\textbackslash}textless 0.05), higher than those in {PNF}5-{PFC} when orally administered in rats. Conclusion: The results suggest that the poor bioavailability of 1 and 2 was due mainly to its poor aqueous solubility and P-glycoprotein (P-gp) efflux. Gelucire®44/14-based solid dispersion could significant improve the bioavailability of lignans.},
	journaltitle = {International Journal of Pharmacology},
	author = {Ma, Hai Qiu and Low, Bin Seng and Chan, Kit Lam and Khan, Nurzalina Abdul Karim},
	date = {2017},
	keywords = {Bioavailability, Lignans, First-pass metabolism, Gelucire®44/14, Gout, Hyperuricemia, P-glycoprotein efflux, Phyllanthus niruri, Solid dispersion},
}

