@article{srirama_hepatoprotective_2012,
	title = {Hepatoprotective Activity of Indian Phyllanthus},
	issn = {13880209},
	doi = {10.3109/13880209.2011.649858},
	abstract = {Context: Phyllanthus (Euphorbiaceae) species are traditionally{\textbackslash}textbackslash nwell-known for their medicinal properties including hepatoprotective{\textbackslash}textbackslash nactivity.{\textbackslash}textbackslash {nObjective}: The study assessed the hepatoprotective and antioxidant{\textbackslash}textbackslash nactivities of 11 Phyllanthus species, P. amarus Schumach., P. urinaria{\textbackslash}textbackslash {nL}., P. debilis Klein ex Willd, P. tenellus Roxb., P. virgatus G. Forst.,{\textbackslash}textbackslash {nP}. maderaspatensis L., P. reticulatus Poir., P. polyphyllus Willd., P.{\textbackslash}textbackslash nemblica L., P. indofischerii Bennet. and P. acidus (L.) Skeels.{\textbackslash}textbackslash {nMaterials} and methods: The dried leaves and stems of each plant species{\textbackslash}textbackslash nwere extracted in methanol and successively in water. The extracts were{\textbackslash}textbackslash nscreened for hepatoprotective activity at a concentration of 50 mu g/{mL}{\textbackslash}textbackslash nagainst tert-butyl hydroperoxide (t-{BH}) induced toxicity in {HepG}2 cells.{\textbackslash}textbackslash {nSeven} extracts from five species that showed hepatoprotective activity{\textbackslash}textbackslash nwere assessed for their 50\% effective concentration ({EC}50) values and{\textbackslash}textbackslash ntheir antioxidant activity using a {DPPH} assay. Phyllanthin and{\textbackslash}textbackslash nhypophyllanthin contents were also determined in these Phyllanthus{\textbackslash}textbackslash nspecies.{\textbackslash}textbackslash {nResults}: The methanol extracts of P. polyphyllus, P. emblica and P.{\textbackslash}textbackslash nindofischeri showed high levels of hepatoprotective activity with {EC}50{\textbackslash}textbackslash nvalues of 12, 19 and 28 mu g/{mL} and {IC}50 of 3.77, 3.38 and 5.8 mu g/{mL}{\textbackslash}textbackslash nfor {DPPH} scavenging activity respectively against an {IC}50 of 3.69 mu{\textbackslash}t