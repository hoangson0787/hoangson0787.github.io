@article{guo_sanchi_2010-1,
	title = {Sanchi ginseng (Panax notoginseng (Burkill) F. H. Chen) in China: Distribution, cultivation and variations},
	volume = {57},
	issn = {09259864},
	doi = {10.1007/s10722-010-9531-2},
