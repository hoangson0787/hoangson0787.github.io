	abstract = {Food nanotechnology encompasses applications related to development of new functional materials (for protection and delivery of food bioactive compounds) and packaging applications (for improved food safety and biosecurity). In recent years, the delivery of micronutrients and nutraceuticals at nanoscale has been widely studied as these systems have the potential to improve the solubility, stability and bioavailability of food bioactives to a greater extent. The nanodelivery systems typically consist of the food bioactive compound encapsulated and stabilized in food grade ingredients, such as lipids, proteins or polysaccharides with diameters ranging from 10 nm to 1000 nm. The solubility and permeability characteristics of the nutrient to be encapsulated play a crucial role in selection of a suitable nanodelivery system. Lipid-based delivery systems such as nanoemulsions, solid lipid nanoparticles, nanoliposomes and micelles are widely studied for the delivery of lipophilic bioactive compounds. On the other hand, the hydrophilic bioactives are delivered through protein; polysaccharide or biopolymer-based colloidal nanosystems, such as hydrogels, nanogels and polymer nanoparticles. This chapter discusses on the techniques for nanoencapsulation of nutrients along with various nanodelivery systems. Further, the nutrient bioavailability enhancement of nanodelivery systems is also discussed in detail.},
	pagetotal = {369-411},
	author = {Bhushani, Anu and Harish, Udayakumar and Anandharamakrishnan, Chinnaswamy},
	date = {2017},
	doi = {10.1016/B978-0-12-804304-2.00010-X},
	note = {Publication Title: Nutrient Delivery},
	keywords = {bioavailability, biopolymer-based delivery, food bioactives, lipid-based delivery, nanodelivery, nanoencapsulation techniques, surfactant-based delivery},
}

