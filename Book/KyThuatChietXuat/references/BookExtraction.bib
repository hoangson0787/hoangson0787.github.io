@article{xie_atractylodis_2013,
	title = {\textit{Atractylodis macrocephalae }Koidz. polysaccharides enhance both serum {IgG} response and gut mucosal immunity},
	volume = {91},
	issn = {01448617},
	url = {http://dx.doi.org/10.1016/j.carbpol.2012.07.083},
	doi = {10.1016/j.carbpol.2012.07.083},
	abstract = {Fifty-six mice were randomly divided into four groups with 14 mice in each. Two groups were subcutaneously injected twice with a foot-and-mouth disease vaccine with 2-week intervals; each of them had been orally administered 0.89\% saline or Atractylodis macrocephalae Koidz. polysaccharides ({RAMPS}) 0.05 g for 4 days before immunization. The rest were not immunized but treated in the same way. One-week after the primary and two weeks after the booster immunization, half in each group were sacrificed to measure serum {IgG} and the parameters for the intestinal mucosal immunity. Results indicated that oral administration of {RAMPS} increased both serum specific {IgG} response and intestinal mucosal immunity as shown by elevated total {sIgA}, {mRNA} expression of {TGF}-??, {IL}-6, {TNF}-??, {IgA}+ cells and intestinal intraepithelial lymphocytes in duodenum. It is suggested that increased serum {IgG} response may be associated with enhanced local mucosal immunity by oral administration of {RAMPS}. ?? 2012 Elsevier Ltd.},
	pages = {68--73},
	number = {1},
	journaltitle = {Carbohydrate Polymers},
	author = {Xie, Feng and Sakwiwatkul, Kedsirin and Zhang, Cenrong and Wang, Yueming and Zhai, Lijuan and Hu, Songhua},
	date = {2013-01},
	pmid = {23044106},
	keywords = {Polysaccharide, Adjuvant, Atractylodis macrocephalae Koidz, Mucosal immunity},
}

@article{li_enhancement_2009-1,
	title = {Enhancement of the immune responses to vaccination against foot-and-mouth disease in mice by oral administration of an extract made from Rhizoma Atractylodis Macrocephalae ({RAM})},
	volume = {27},
	issn = {0264410X},
	doi = {10.1016/j.vaccine.2009.02.002},
	abstract = {This study was designed to evaluate the effects of oral administration of a water extract made from the Rhizoma Atractylodis Macrocephalae ({RAM}) on the immune responses in mice immunized with {FMDV} type O vaccine. Thirty-five {ICR} mice were randomly divided into five groups with seven animals in each group, and orally administered daily for 4 days at a dose equivalent to 0, 0.0625, 0.125, 0.25 or 0.5 g of dried {RAM}, respectively. After that, the animals were subcutaneously immunized twice with {FMDV} vaccine at 2-week intervals. Blood samples were collected 3 weeks after boosting for measurement of {FMDV}-specific {IgG} titers and the {IgG} subclasses, lymphocyte proliferation as well as production {IL}-5 and {IFN}-??. Results indicated that serum {FMDV}-specific {IgG} titers and the {IgG} subclass responses were significantly enhanced in mice orally administered {RAM} at the dose of 0.25 or 0.5 g when compared with the control group (P {\textbackslash}textless 0.05). Splenocyte proliferation in response to Con A and {LPS} and production of {IL}-5 and {IFN}-?? by splenocytes were also significantly enhanced (P {\textbackslash}textless 0.05). Considering the immunomodulatory effect and safety of {RAM} demonstrated in this study, this herb deserves further investigation to evaluate its potential improvement of {FMD} vaccination in other animals such as pigs, goats and cattle. ?? 2009 Elsevier Ltd. All rights reserved.},
	pages = {2094--2098},
	number = {15},
	journaltitle = {Vaccine},
	author = {Li, Ruili and Sakwiwatkul, Kedsirin and Yutao, Li and Hu, Songhua},
	date = {2009},
	pmid = {19356611},
	keywords = {Adjuvants, Foot-and-mouth disease, Rhizoma Atractylodis Macrocephalae},
}

