@article{wei_junxian_study_1992-1,
	title = {{STUDY} {AND} {DEVELOPMENT} {OF} {SANCHI}({PANAX} {NOTOGINSENG} ({BURK}.) F. H. {CHEN})–《Natural Product Research and Development》1992年03期},
	volume = {03},
	url = {http://en.cnki.com.cn/Article_en/CJFDTOTAL-TRCW199203012.htm},
	abstract = {Sanchi is the roots of Panax notoginseng (Burk. ) F. H. Chen (Araliaceae). Its activitied of homostatie, promoting of blood circulation, relieving pain and cure of bleeding Wound and trauma are recorded in traditional Materia Medica and get used to medical practice. Steamed Sanchi are used in folks as haemotic tonics. For the purpose to extend its uses, we have carried on studies of Sanchi. Through studies of its chemical constituents, We primarily find out the chemical structure of the main saponins in different part of this plant also discovered some compounds with new structure and constituents which have not been reported. On purpose of comprehensive utilization we have furnished reliable reports of che、mical studies of its rootlets, stems, leaves and fruit stalks which have not been used in medical practice and suggested to use them as raw materials for preparation of pharmaceuticals and saponins. A newer, simpler and easier method for extraction and isolation of saponins have also been carried out. This method leads to prepare standard ginsenoside-Rg\_1 and-Rb\_1 from Sanchi which are furnished as standards in qulitative control of Panax ginseng and P. notoginseng and their preparations. Some pharmacological activities of Sanchi have also been studied, such as the effects of saponins from Sanchion cardiovascular system; cerebrovaseular system; central nervious system; blood and homopoietic system; protection against shock; metabolism; experimental Liver injury and anti-inflammatory activity etc. In according with those studies some new drugs for treatment of cardiovascular, cerebrovascular diseases and a new drug for treatment of hemicrania and hypnotic effect has been developed. Those {neW} drugs are menufactured by pharmaceutical manufacturers and supplied in market which are well received by patients.},
	journaltitle = {Natural Product Research and Development},
	author = {Wei Junxian, Du Yuanzhong},
	urldate = {2018-07-08},
	date = {1992},
}

