@article{feldman_diabetic_2019,
	title = {Diabetic Neuropathy},
	volume = {5},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7096070/},
	doi = {10.1038/s41572-019-0097-9},
	abstract = {The global epidemic of prediabetes and diabetes has led to a corresponding epidemic of complications of these disorders. The most prevalent complication is neuropathy, of which distal symmetric polyneuropathy (for the purpose of this Primer, referred to as diabetic neuropathy) is very common. Diabetic neuropathy is a loss of sensory function beginning distally in the lower extremities that is also characterized by pain and substantial morbidity. Over time, at least 50\% of individuals with diabetes develop diabetic neuropathy. Glucose control effectively halts the progression of diabetic neuropathy in patients with type 1 diabetes mellitus, but the effects are more modest in those with type 2 diabetes mellitus. These findings have led to new efforts to understand the aetiology of diabetic neuropathy, along with new 2017 recommendations on approaches to prevent and treat this disorder that are specific for each type of diabetes. In parallel, new guidelines for the treatment of painful diabetic neuropathy using distinct classes of drugs, with an emphasis on avoiding opioid use, have been issued. Although our understanding of the complexities of diabetic neuropathy has substantially evolved over the past decade, the distinct mechanisms underlying neuropathy in type 1 and type 2 diabetes remains unknown. Future discoveries on disease pathogenesis will be crucial to successfully address all aspects of diabetic neuropathy, from prevention to treatment.},
	pages = {42},
	number = {1},
	journaltitle = {Nature reviews. Disease primers},
	author = {Feldman, Eva L and Callaghan, Brian C and Pop-Busui, Rodica and Zochodne, Douglas W and Wright, Douglas E and Bennett, David L and Bril, Vera and Russell, James W and Viswanathan, Vijay},
	date = {2019},
}

