@article{qiao_high-performance_2013,
	title = {High-Performance Anion-Exchange Chromatography Coupled with Diode Array Detection for the Determination of Dencichine in Panax Notoginseng and Related Species},
	volume = {36},
	issn = {16159306},
	doi = {10.1002/jssc.201300334},
	abstract = {A high-performance anion-exchange chromatography coupled with diode array detection method was developed for the determination of dencichine in Panax notoginseng and related species. The analysis was performed on an Eprogen Synchropak {WAX} column (4.6 × 250 mm, 6 μm) with 50 {mM} {NaH} 2 {PO} 4 aqueous solution isocratic elution. The method was validated in terms of linearity, sensitivity, precision, stability, and accuracy. It was found that the calibration curve for dencichine showed good linearity (R 2 = 0.9999) within the test range. The {LOD} and {LOQ} were 0.77 and 3.06 ng, respectively. The {RSD} for intra- and interday repeatability was 0.2 and 0.5\%, respectively. The test solution of dencichine is stable at least for three days at room temperature and for seven days at 4°C. The mean recovery of dencichine was 102.0\%. The established method was successfully applied to determine dencichine in the raw root of P. nogoginseng, P. ginseng, and P. quinquefolium as well as the steamed root of P. notoginseng. Compared with previous reports, this method is sensitive, selective, and accurate, which is helpful to evaluate the quality of P. notoginseng and related species. © 2013 {WILEY}-{VCH} Verlag {GmbH} \& Co. {KGaA}, Weinheim.},
	number = {15},
	journaltitle = {Journal of Separation Science},
	author = {Qiao, C.-F. and Liu, X.-M. and Cui, X.-M. and Hu, D.-J. and Chen, Y.-W. and Zhao, J. and Li, S.-P.},
	date = {2013},
	keywords = {Panax, Anion-exchange chromatography, Dencichine, traditional Chinese medicine},
}

