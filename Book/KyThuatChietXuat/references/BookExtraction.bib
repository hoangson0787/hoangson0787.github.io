@article{johnson_infrared_1965-1,
	title = {Infrared Spectra of Solid 1:1 Pyridine-Benzoic Acid Complexes; the Nature of the Hydrogen Bond as a Function of the Acid-Base Levels in the Complex1},
	volume = {69},
	issn = {0022-3654},
	url = {http://dx.doi.org/10.1021/j100885a013},
	doi = {10.1021/j100885a013},
	abstract = {The spectroscopic properties of 18 strongly hydrogen-bonded pyridine-benzoic acid adducts have been correlated with the {ApK} value of the complex. As the critical {DpK} for proton transfer is approached from either side of the critical region, increased broadening of the n-{NH} or n-{OH} and n-C=O bands is observed. Also, strong background absorption below 1200 cm-1 occurs. At the critical {DpK} value, discontinuous changes take place in the n-C=O and n-C-O regions of the spectrum, indicating that the adducts are either predominantly ionized or are predominantly un-ionized. In all of the complexes, either no band above 1700 cm-1 could be ascribed to n-{OH} or n-{NH}, or two bands near 1900-2000 and 2500-2600 cm-1 could be attributed to these modes. These two situations have been interpreted as corresponding to single-minimum- or near single-minimum-type and double-minimum-type hydrogen bonds, respectively. The single-minimum-type, hydrogen-bonded complex occurs only when its {DpK} is close to the critical {DpK} for proton transfer as determined from the n-C-O and n-C=O bands.},
	pages = {74--86},
	journaltitle = {The Journal of Physical Chemistry},
	author = {Johnson, S L and Rumon, K A},
	date = {1965},
}

