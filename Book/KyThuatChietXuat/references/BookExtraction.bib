@article{he_antioxidant_2012,
	title = {Antioxidant, Antiproliferative, and Pro-Apoptotic Activities of a Saponin Extract Derived from the Roots of {\textbackslash}{emphPanax}{\textbackslash}emph Notoginseng (Burk.) F.H. Chen},
	volume = {15},
