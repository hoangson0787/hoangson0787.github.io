	number = {3},
	journaltitle = {Journal of pharmacy \& pharmaceutical sciences [electronic resource] : a publication of the Canadian Society for Pharmaceutical Sciences, Societe canadienne des sciences pharmaceutiques},
	author = {Quintero, A. and Pelcastre, A. and Solano, J. D.},
